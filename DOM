etsi.src ="http://placekitten.com/420/300";
etsi.alt="kuva 1";

const next = document.getElementById('img')[1];
etsi.src ="http://placekitten.com/420/300";
etsi.alt="kuva 1";

Tai

function funktio() {
    var kuva = document.createElement("img");
    kuva.setAttribute("src", "http://placekitten.com/420/300");
    document.body.appendChild(kuva);

}


