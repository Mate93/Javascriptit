Tehtävä 1

<script>

     luku = prompt('Anna kokonaisluku');

     kertoma = 1;

    while(luku != 0){
        kertoma = kertoma * luku;
        luku = luku - 1;
    }

    document.write(kertoma);
</script>

tehtävä 2           //Ei vielä TOIMI
     const count = prompt('Anna kokonaisluku');


  document.write(Math.sqrt(count).toFixed(2));

  let miinus =math.abs(count);
  while (miinus < 0){

    document.write("Loop stopped!");
    document.write(Math.sqrt(miinus).toFixed(2));
  }


