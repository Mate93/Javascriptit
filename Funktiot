teht 2
'use strict';
let x;
x= prompt('Anna galloonan määrä');
    function muunnos(galloona=3.785) {

        return (3.785*galloona);

    }

document.write(muunnos(x));

teht 3
'use strict';
let a= prompt('Anna arvo');

    function itseisarvo(arvo) {
        return Math.abs(a);
    }

    document.write(itseisarvo(a));
teht 4
 'use strict';

  const eka = prompt('Anna eka luku');
  const toka = prompt('Anna eka luku');
  const kolmas = prompt('Anna eka luku');

  function summa(luku1, luku2, luku3) {

    return Math.max(luku1, luku2, luku3);
  }

  document.write(summa(eka, toka, kolmas));
  
  teht 6
  
      'use strict';
  function vastaluvuksi(num)
  {
    return -Math.abs(num);
  }

  document.write(vastaluvuksi(20));
  document.write(vastaluvuksi(25));
  document.write(vastaluvuksi(32));
  document.write(vastaluvuksi(8));

  
